{
  "name": "@byu-oit/express-logger",
  "version": "0.2.1",
  "description": "Default express logging middleware to match BYU app dev logging standards",
  "main": "dist/express-logger.js",
  "types": "dist/express-logger.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "coverage": "npm run test -- --coverage || exit 0",
    "lint": "ts-standard | snazzy",
    "lint:fix": "ts-standard --fix | snazzy",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byu-oit/express-logger.git"
  },
  "author": "Brigham Young University",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/byu-oit/express-logger/issues"
  },
  "homepage": "https://github.com/byu-oit/express-logger#readme",
  "dependencies": {
    "@byu-oit/logger": "^0.3.1",
    "pino-http": "^5.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tsconfig/node10": "^1.0.8",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/pino": "^6.3.9",
    "@types/pino-http": "^5.4.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "express": "^4.17.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "pino-pretty": "^5.1.2",
    "rimraf": "^3.0.2",
    "snazzy": "^9.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-standard": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  }
}
